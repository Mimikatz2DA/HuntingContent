import re

def extract_unique_ips(file_path):
    # Regular expression for IPv4 addresses
    ip_pattern = r'\b(?:\d{1,3}\.){3}\d{1,3}\b'
    ip_addresses = []

    try:
        with open(file_path, 'r') as file:
            content = file.read()
            # Find all matches and convert to set to remove duplicates
            ip_addresses = list(set(re.findall(ip_pattern, content)))
    except FileNotFoundError:
        print(f"Error: File '{file_path}' not found.")
    except Exception as e:
        print(f"Error: {e}")

    return ip_addresses

# Example usage
file_path = 'ips.txt'  # Replace with your file path
ips = extract_unique_ips(file_path)
for ip in ips:
    print(ip)
